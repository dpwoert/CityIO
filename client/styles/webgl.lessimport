.visualization {
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;

	cursor: none;
}

#cursor{
	position: fixed;
	top: 0;
	left: 0;

	width: 20px;
	height: 20px;

	margin-left: -10px;
	margin-top: -10px;

	background-image: url(/main/cursor.png);
	background-size: contain;

	cursor: none;
}